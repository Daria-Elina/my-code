def calculate(a, b, operation):
   result = None
   if operation == '+':
       result = sum(a, b)
   elif operation == '-':
       result = subtract(a, b)
   elif operation == '/':
       result = divide(a, b)
 elif operation == '*':
       result = multiply(a, b)
 # хххх
   elif operation == '^' or operation == '**':
       result = pow(a, b)
else:
print('ххх')
return result
